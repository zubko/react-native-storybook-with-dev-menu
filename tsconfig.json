{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src/",
    "paths": {
      "@assets/*": [
        "assets/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@contexts/*": [
        "contexts/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@modules/*": [
        "modules/*"
      ],
      "@navigators/*": [
        "navigators/*"
      ],
      "@screens/*": [
        "screens/*"
      ],
      "@app-types/*": [
        "types/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    }
  }
}
